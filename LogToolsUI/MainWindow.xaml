                </Label>
            </Expander>
            <DataGrid DockPanel.Dock="Bottom" 
                      RowHeight="25"
                      MaxColumnWidth="300"
                      ItemsSource="{Binding}"
                      Name="dataGrid">
            </DataGrid>
        </DockPanel>
    </Grid>
</Window>
